<?php

namespace Km;

/**
 * Avatar model for the rider avatars
 *
 * Every rider has 4 avatars of 4 different sizes.
 * Each size is a constant in this model.
 *
 * @author RJ Garcia <rj@bighead.net>
 * @package Ewt Model
 */
class Avatar extends \Krak\Model
{
	const XLARGE	= '1024x1024';
	const LARGE		= '420x480';
	const MED		= '140x160';
	const SMALL		= '92x105';

	protected $created_field	= 'created_at';
	protected $updated_field	= 'updated_at';
}
