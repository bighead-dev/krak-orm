{
    "name": "krak/orm",
    "description": "Simple DataMapper ORM",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/bighead-dev/krak-orm",
    "keywords": [
        "datamapper",
        "orm",
        "model"
    ],
    "authors": [
        {
            "name": "RJ Garcia",
            "email": "rj@bighead.net"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "doctrine/dbal": "2.5.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "psr-4": {
            "Krak\\Orm\\" : "lib"
        },
        "files" : ["lib/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Krak\\Tests\\" : "tests"
        }
    },
    "minimum-stability": "dev"
}
